
@{
    ViewBag.Title = "Nuevo Empleado";
    Layout = "~/Views/Shared/Menu.cshtml";
}
<style type="text/css" media="screen and (max-width:700px)">
    .FormBox {
        width: 90%;
        padding: 5%;
        border: 4px outset;
        margin-top: 3%;
        background-color: white
    }

    .txt {
        text-align: center !important;
        width: 100%;
    }
    hr.custom {
    border: 0;
    height: 0; /* Firefox... */
    box-shadow: 0 0 20px 3px black;
    
}
        hr.custom:after { /* Not really supposed to work, but does */
            content: "\00a0"; /* Prevent margin collapse */
        }

    [type="file"] {
       display:none !important;
    }
    .ImgPrueba {
        width: 100%;
    }
</style>
<script>

    function getBase64(file) {
        var reader = new FileReader();
        reader.readAsDataURL(file);
        reader.onload = function () {
            $("#FotoString").val(reader.result)
            $("#ImgPrueba").attr("src", reader.result);
        };
        reader.onerror = function (error) {
            console.log('Error: ', error);
        };
    }
    $(function () {
        $("#Foto").change(function () {
            var file = document.querySelector('#Foto').files[0];
            getBase64(file) // prints the base64 string
        });
    })

    function Confirmar() {
        if (window.confirm("Desea aceptar?")) {
            alert("Aceptado");
        }
    }
</script>

<!-- Contenido -->

<div data-theme="b" align="center">
    <div class="FormBox">
        <h1>Informacion</h1>

        <label>Nombre:</label>
        <input class="txt" type="text" name="Nombre" readonly>

        <label>Puesto:</label>
        <input class="txt" type="text" value="" name="Puesto" readonly>

        <label>Telefono:</label>
        <input class="txt" type="text" value="" name="Telefono" readonly>

        <label>Direccion:</label>
        <input class="txt" type="text" value="" name="Direccion" readonly>

        <label>Salario:</label>
        <input class="txt" type="number" step="0.01" name="Salario" value="0.00">

        <select name="Ubicacion" data-native-menu="false">
            <option>Ubicacion</option>
            <option value="Cyber 1">Cyber 1</option>
            <option value="Cyber 2">Cyber 2</option>
        </select>

        <select name="Turno" data-native-menu="false">
            <option selected>Turno</option>
            <option value="Matutino">Matutino</option>
            <option value="Vespertino">Vespertino</option>
            <option value="Completo">Completo</option>
        </select>

        <input class="inputfile" id="Foto" type="file" accept="image/*"><br>
        <label style="font-size:x-large;cursor: pointer;" for="Foto"><span style="padding:5%" class="badge badge-secondary">Selecciona una foto</span></label><br><br>
        <input name="FotoString" id="FotoString" type="hidden"><br>
        <img class="ImgPrueba" id="ImgPrueba" src="data:image/png;base64," alt=""><br><br>


        <br /><br />
        <hr class="custom" />
        <br /><br />

        <h1>Usuario</h1>
        <label>Usuario:</label>
        <input class="txt" type="text" value="" name="text-basic" readonly>

        <label>Contraseña:</label>
        <input class="txt" type="text" value="" name="text-basic" readonly>

        <a href="#" onclick="Confirmar()" class="ui-btn ui-icon-check ui-btn-icon-left">Aceptar</a>
    </div>

</div>
<!-- Contenido -->
